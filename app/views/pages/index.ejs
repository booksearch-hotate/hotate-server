<!DOCTYPE html>
<html>
  <head>
    <%- include('./../components/head') %>
    <link rel="stylesheet" href="<%= pageData.origin %>/css/index.css" />
    <link rel="stylesheet" href="<%= pageData.origin %>/css/admin/recommendation/recommendation.css" />
    <meta name="csrfToken" content="<%= pageData.csrfToken %>">
  </head>
  <body>
    <%- include('./../components/navbar') %>
    <div class="home-search-box">
      <form action="/search" method="get">
        <input type="hidden" name="type" value="book">
        <div class="form-group search-input-main-box">
          <input type="text" class="search-input-main" id="search-input" name="search" placeholder="Search" required>
        </div>
      </form>
      <div class="detail-search-box mt-1">
        <a onclick="viewSearchBox()">
          <p>詳細設定</p>
        </a>
      </div>
    </div>

    <%- include('./../components/recommendation-list') %>
    <%- include('./../components/input-js') %>
    <%- include('./../components/state-alert', {pageData}) %>
    <script src="<%= pageData.origin %>/js/getImgLinks.js"></script>
  </body>
</html>