<%#
ページネーション
%>
<nav aria-label="Page navigation example">
  <ul class="pagination justify-content-center">
    <% if (paginationData.pageCount >= 1) { %>
      <li class="page-item">
        <a class="page-link" href="<%= url %>page=<%= paginationData.pageCount %>" aria-label="Previous">
          <span aria-hidden="true">&laquo;</span>
        </a>
      </li>
    <% } %>
    <% for (let i = paginationData.pageRange.min; i <= paginationData.pageRange.max; i++) { %>
      <li class="page-item <% if (i - 1 === paginationData.pageCount){ %>active<% } %>">
        <a class="page-link" href="<%= url %>page=<%= i %>"><%= i %></a>
      </li>
    <% } %>
    <% if (paginationData.pageCount < paginationData.totalPage - 1) { %>
      <li class="page-item">
        <a class="page-link" href="<%= url %>page=<%= paginationData.pageCount + 2 %>" aria-label="Next">
          <span aria-hidden="true">&raquo;</span>
        </a>
      </li>
    <% } %>
  </ul>
</nav>